{
  "name": "rz-methods",
  "version": "2.1.4",
  "description": "常用js方法",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.js",
  "umd": "dist/umd/index.min.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build:cjs": "tsc -b ./tsconfig.cjs.json",
    "build:es": "tsc -b ./tsconfig.esm.json",
    "build:test": "tsc -b ./tsconfig.test.json",
    "build:types": "tsc -b ./tsconfig.types.json",
    "build:umd": "cross-env NODE_ENV=development rollup -c",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c",
    "build": "npm run clean && npm-run-all build:cjs build:es build:types build:umd build:umd:min",
    "clean": "rm -rf dist",
    "tsc": "tsc",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Roc-zhou/methods.git"
  },
  "author": "roc_zhou",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Roc-zhou/methods/issues"
  },
  "homepage": "https://github.com/Roc-zhou/methods#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.19",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.3.1"
  },
  "keywords": [
    "util",
    "methods"
  ],
  "dependencies": {
    "@jest/core": "^27.2.3",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "^8.2.5",
    "cross-env": "^7.0.3",
    "rollup": "^2.57.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.4.3"
  },
  "jest": {
    "rootDir": "tests",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "peerDependencies": {}
}
